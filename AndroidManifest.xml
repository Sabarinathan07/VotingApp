<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sabari.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <shape android:shape="rectangle" >
        <solid android:color="@android:color/transparent" />

        <corners
            android:bottomLeftRadius="8dp"
            android:bottomRightRadius="8dp"
            android:topLeftRadius="8dp"
            android:topRightRadius="8dp" />

        <stroke
            android:width="1dp"
            android:color="#9c9a9b" />
    </shape>

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sabari.demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sabari.demo.SignIn"
            android:label="@string/title_activity_sign_in" >
        </activity>
        <activity
            android:name="com.sabari.demo.LogIn"
            android:label="@string/title_activity_log_in" >
        </activity>
        <activity
            android:name="com.sabari.demo.LastActivity"
            android:label="@string/title_activity_last" >
        </activity>
        <activity
            android:name="com.sabari.demo.Adapter"
            android:label="@string/title_activity_adapter" >
        </activity>
    </application>

</manifest>
